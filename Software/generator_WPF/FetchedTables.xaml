<Window x:Class="generator_WPF.FetchedTables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:generator_WPF"
        mc:Ignorable="d"
        Title="Fetched Tables" Height="450" Width="600" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen" Icon="/icon.png">
    <Grid>
        <DataGrid x:Name="dgTables" AutoGenerateColumns="True" CanUserAddRows="False" Margin="10,40,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="300" Width="530"></DataGrid>
        <Label Content="Fetched Tables: " HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top"></Label>
        <Button x:Name="btnChooseTable" Content="Choose Table(s)"  HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Click="btnChooseTable_Click" Margin="450,383,0,0"></Button>
    </Grid>
</Window>
